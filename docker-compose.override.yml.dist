version: '3.8'

services:
  busyboxk8s:
    environment:
      BUSYBOX_HOME: ${BUSYBOX_HOME}
      BUSYBOX_ENV_FILE: ${BUSYBOX_ENV_FILE}
      NAMESPACE_DEFAULT: ${NAMESPACE_DEFAULT}
    volumes:
      - ./config/aws/config:/root/.aws/config
      - ./config/aws/credentials:/root/.aws/credentials
      - .:/busyboxk8s
